<?xml version="1.0"?>
<block>
  <name>Message Vector Sink</name>
  <key>message_file_msg_vector_sink</key>
  <category>message_file</category>
  <import>import message_file</import>
  <make>message_file.msg_vector_sink($vlen)</make>

	<param>
		<name>Vec Length</name>
		<key>vlen</key>
		<value>1</value>
		<type>int</type>
	</param>

  <sink>
    <name>print_pdu</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <doc>
      Abosrbs messages and store the cdr as a vector
      like the block_vector_sink.
  </doc>
</block>
